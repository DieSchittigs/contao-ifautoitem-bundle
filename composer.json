{
    "name": "dieschittigs/contao-ifautoitem-bundle",
    "description": "Add checkbox to elements and chech if there is an auto Item used, then show or hide",
    "type": "contao-bundle",
    "keywords":["contao","autoitem","contentelements","bundle"],
    "require": {
        "contao/core-bundle": "^4.4"
    },
    "license": "MIT-0",
    "authors": [
        {
            "name": "Martin Schittig",
            "email": "martin@dieschittigs.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "DieSchittigs\\IfautoitemBundle\\" : "src/"
        },
        "classmap": [
            "src/Resources/contao/"
		],
		"exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/"
		]
    },
    "extra": {
		"contao-manager-plugin": "DieSchittigs\\IfautoitemBundle\\ContaoManager\\Plugin"
	}
}
